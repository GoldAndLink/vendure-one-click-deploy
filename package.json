    "@pinelab/vendure-plugin-order-pdfs": "^1.0.0",
    "@vendure/admin-ui-plugin": "^3.1.0",
    "@vendure/asset-server-plugin": "^3.1.0",
    "@vendure/cli": "^3.1.0",
    "@vendure/core": "^3.1.0",
    "@vendure/create": "^3.1.0",
    "@vendure/email-plugin": "^3.1.1",
    "dotenv": "16.4.5",
    "pg": "8.13.0",
    "typescript": "5.6.2"
  },
  "devDependencies": {
    "@types/react": "19.0.9",
    "@vendure/ui-devkit": "3.1.3",
    "concurrently": "9.0.1",
    "ts-node": "10.9.2"
  }
